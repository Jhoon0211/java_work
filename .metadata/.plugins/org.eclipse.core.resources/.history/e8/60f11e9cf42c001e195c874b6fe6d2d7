package oops_p;

class AAA{
	int a = 10;
	int b = 20;
	int sa = 1111;
	
	public AAA() {
		
		System.out.println("AAA 기본 생성자");
	}
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "AAA [a="+a+", b="+b+","sa="+sa+"]";
	}
}

public class instanceMain {

	public static void main(String[] args) {
			
		AAA a1 = new AAA();
		System.out.println(a1);
	}

	

}
